{
    "name": "auth-contrib/php-auth-library",
    "description": "Port of GoDaddy's Java JWT authentication library following the GoDaddy authentication standards.",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Milos Colakovic",
            "email": "mcolakovic@godaddy.com"
        },
        {
            "name": "Jeff Channell",
            "email": "jchannell@godaddy.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "GoDaddy\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GoDaddy\\Auth\\Tests\\": "tests/"
        }
    },
    "bin": ["bin/main"],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "ext-openssl": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.6"
    }
}
